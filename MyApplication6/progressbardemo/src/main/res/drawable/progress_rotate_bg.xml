<?xml version="1.0" encoding="utf-8"?>
<rotate xmlns:android="http://schemas.android.com/apk/res/android"
    android:fromDegrees="0"
    android:toDegrees="360"
    android:pivotX="50%"
    android:pivotY="50%">
    <shape
        android:shape="ring"
        android:useLevel="false">
        <gradient
            android:startColor="#f19ec2"
            android:endColor="#f5d4d9"
            android:centerColor="#f9cce2">
        </gradient>
    </shape>

</rotate>